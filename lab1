import sys
def reverse(s):
  return s[::-1]

def int2binary(x):
    y = x
    if y == 0:
        print("0")
        return 0
    string2 = ""

    while y >= 1:
        if y % 2 == 0:
            string2 = string2 + "0"
            y = y/2
        else:
            string2 = string2 + "1"
            y = y/2
            y = y-0.5
    string2 = reverse(string2)
    print(string2)

int2binary(7)
int2binary(378)


def parseMessage(inputString):

    listS = inputString.split("\r\n")
    return listS


